<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="namesayer.SaveScreenController">
    <children>
        <TextField fx:id="nameText" layoutX="54.0" layoutY="167.0" prefHeight="31.0" prefWidth="292.0" />
        <Label layoutX="111.0" layoutY="51.0" text="Save Playlist">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Label>
        <Label layoutX="183.0" layoutY="108.0" />
        <Button fx:id="saveButton" layoutX="298.0" layoutY="282.0" mnemonicParsing="false" onAction="#saveButtonClicked" text="Save" />
        <Button fx:id="cancelButton" layoutX="229.0" layoutY="282.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel" />
      <Label alignment="CENTER" layoutX="55.0" layoutY="137.0" prefHeight="20.0" prefWidth="292.0" text="Enter a name for the new playlist:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
